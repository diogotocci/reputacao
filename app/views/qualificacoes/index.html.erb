<h2>Listar Qualificacoes</h2>
<table class="table table-striped">
  <tr>
    <th>Cliente</th>
    <th>Proprietario</th>
    <th>Nota</th>
    <th></th>
    <th></th>
  </tr>

<% @qualificacoes.each do |qualificacao| %>
  <tr>
    <td><%= qualificacao.cliente.nome %></td>
    <td><%= qualificacao.proprietario.nome %></td>
    <td><%= qualificacao.nota %></td>
    <td>
		<%= link_to 'Mostrar', qualificacao %> |
		<%= link_to 'Editar', edit_qualificacao_path(qualificacao) %> | 
		<%= link_to 'Deletar', qualificacao, confirm: 'Confirma?', method: :delete %>
	</td> 
  </tr>
<% end %>
</table>
<table>
	<tr>
		<td>
			<a href='/qualificacoes/new' class='btn btn-small pull-right'>Inserir Qualificacao</a><br/>
		</td>
	</tr>
</table>